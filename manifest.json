{
  "name": "fresources",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Injects a download button and removes ad blocker notice on fresources",
  "homepage_url": "https://github.com/dvishal485/",
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo.png"
  },
  "permissions": [
    "contextMenus",
    "https://v3.fresources.tech/*",
    "https://fresources.tech/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://v3.fresources.tech/*",
        "https://fresources.tech/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}